{
  "name": "com.doctorhandbook.app",
  "displayName": "Doctor Handbook",
  "version": "1.0.0",
  "description": "Doctor Handbook Application.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env DEBUG=webpack NODE_ENV=development BABEL_ENV=buildscript node --max_old_space_size=1024 -r babel-register .compiler/dev.es6",
    "lint": "node_modules/.bin/eslint --fix --ext .js,.vue -f node_modules/eslint-friendly-formatter src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "platform:android": "node_modules/.bin/cordova platform add android --nofetch --nosave",
    "clean:www": "shx rm -rf ./www/* && shx touch ./www/.gitkeep",
    "clean:project": "shx rm -rf ./platforms && shx rm -rf ./plugins rm -rf ./node_modules",
    "clean:platform": "node_modules/.bin/cordova clean",
    "build:www": "cross-env DEBUG=webpack NODE_ENV=production BABEL_ENV=buildscript node --max_old_space_size=1024 -r babel-register .compiler/build.es6",
    "build:debug": "node_modules/.bin/cordova build android --debug",
    "build:release": "cross-env ORG_GRADLE_PROJECT_cdvMinSdkVersion=22 ORG_GRADLE_PROJECT_cdvTargetSdkVersion=22 node_modules/.bin/cordova build android --release",
    "copy": "shx cp res/google-services.json platforms/android/app",
    "build": "npm run build:debug",
    "device:install": "adb install -r platforms/android/app/build/outputs/apk/debug/app-debug.apk",
    "device:uninstall": "adb uninstall com.doctorhandbook.app"
  },
  "author": "Doctorhandbook Team",
  "license": "MIT",
  "engines": {
    "node": ">8.0.0"
  },
  "browserslist": [
    "last 5 chrome version"
